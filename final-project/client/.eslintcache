userService\n            .editUser(user_id, this.state)\n            .then(() => console.log(\"good\"))            \n            .catch((err) => console.log(err))\n    }\n    \n    render() {\n        return (\n            <>\n                <Container>\n                    <Row> \n                        {  \n                             this.state.user ?\n                                \n                                this.state.user.board.map((elm, idx) => {\n                                    return (\n                                    <Col lg={3} className=\"image-container\" key={idx}>\n                                        <Card className=\"image-card\">  \n                                            <Card.Img src={elm} variant=\"top\" />                                         \n                                        </Card>\n                                        <Button onClick={() => this.getImages(elm)} variant=\"dark\" size=\"sm\" className=\"create-btn mb-4\">Delete</Button>    \n                                    </Col>\n                                    )\n                                } )\n                                \n                             : null \n                        } \n                    </Row>\n                </Container>\n            </>      \n        )\n    }\n}\n\nexport default Board","/Users/SaraMartz/Desktop/final-project-app/final-project/client/src/components/pages/Home/Home.js",[],"/Users/SaraMartz/Desktop/final-project-app/final-project/client/src/components/pages/Account/Follows.js",[],{"ruleId":"160","replacedBy":"161"},{"ruleId":"162","replacedBy":"163"},{"ruleId":"164","severity":1,"message":"165","line":69,"column":51,"nodeType":"166","endLine":69,"endColumn":56},{"ruleId":"160","replacedBy":"167"},{"ruleId":"162","replacedBy":"168"},{"ruleId":"169","severity":1,"message":"170","line":2,"column":29,"nodeType":"171","messageId":"172","endLine":2,"endColumn":33},{"ruleId":"173","severity":2,"message